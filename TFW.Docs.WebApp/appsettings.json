{
  "Logging": {
    "LogLevel": {
      "Default": "Information",
      "Microsoft": "Warning",
      "Microsoft.Hosting.Lifetime": "Information"
    }
  },
  "AllowedHosts": "*",

  "WebAppSettings": {
    "Author": "TNT",
    "ApiBase": "https://localhost:44357",
    "CookiePersistenceHours": 1
  },

  "AppSettings": {
    "SupportedCultureNames": [
      "en",
      "vi"
    ]
  },

  "JwtSettings": {
    "Issuer": "TFW.Docs",
    "Audiences": [ "TFW.Docs" ],
    "SecretKey": "{secret}"
  }
}
