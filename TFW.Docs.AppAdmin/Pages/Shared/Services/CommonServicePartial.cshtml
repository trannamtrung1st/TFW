@{
}

<script src="/dist/js/commonService.js"></script>
<script>
    function getCommonService() {
        return CommonService({
            defaultConfirmBtnText: '@AppResLocalizer[AppResources.DefaultConfirmBtnText]',
            defaultConfirmTitle: '@AppResLocalizer[AppResources.DefaultConfirmTitle]',
            defaultErrorTitle: '@AppResLocalizer[AppResources.DefaultErrorTitle]',
            defaultSuccessTitle: '@AppResLocalizer[AppResources.DefaultSuccessTitle]',
            defaultOkBtnText: '@AppResLocalizer[AppResources.DefaultOkBtnText]',
            defaultErrorHtml: '@AppResLocalizer[AppResources.DefaultErrorHtml]',
        });
    }
</script>